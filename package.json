{
	"name": "@iqrok/sftp-watch",
	"version": "0.5.0",
	"type": "module",
	"main": "exports/index.cjs",
	"module": "exports/index.mjs",
	"types": "exports/index.d.ts",
	"exports": {
		".": {
			"types": "./exports/index.d.ts",
			"import": "./exports/index.mjs",
			"require": "./exports/index.cjs"
		}
	},
	"files": [
		"src/*.cc",
		"src/*.cpp",
		"src/*.hpp",
		"src/*.h",
		"main.js",
		"CMakeLists.txt"
	],
	"scripts": {
		"sample": "node sample/index.js",
		"rebuild:static": "cmake-js rebuild --CDUSE_STATIC_LIBS=1 --config Release",
		"rebuild:dbg": "cmake-js rebuild -D CMAKE_BUILD_TYPE=Debug",
		"rebuild": "cmake-js rebuild",
		"build:dbg": "cmake-js build --debug",
		"build": "cmake-js build",
		"compile": "cmake-js compile",
		"clean": "cmake-js clean",
		"postinstall": "npm run compile"
	},
	"devDependencies": {
		"cmake-js": "^7.3.1"
	},
	"dependencies": {
		"node-addon-api": "^8.5.0"
	}
}
